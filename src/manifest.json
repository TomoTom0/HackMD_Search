{
  "name": "HackMDsearch",
  "description": "This extension push your notes in HackMD to GitHub and enable you to search contents via github.",
  "version": "0.1.2",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://hackmd.io/*"],
    "js": ["jquery-3.5.1.min.js", "content_script.js"]
  }],
  "permissions":["*://api.github.com/*", "*://github.com/*", "storage", "unlimitedStorage"],
  "web_accessible_resources":[".env"],
  "options_page": "options.html"
}